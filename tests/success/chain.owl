locality alice

name d : nonce @ alice

name psk3 : dualkdf {salt info. 
    True -> strict enckey Name(d)
} @ alice

name psk2 : dualkdf {ikm info. 
    True -> strict kdf {salt info. 
        True -> strict enckey Name(d)
    }
} @ alice

name psk1 : kdf {ikm info.
    True -> strict kdf {salt info. 
        True -> strict enckey Name(d)
    }
} @ alice

corr [psk1] /\ [psk2] /\ [psk3] ==> [d]

def main() @ alice : Unit = 
    corr_case psk1 in 
    corr_case psk2 in 
    corr_case psk3 in 
    let ck1 = kdf<0;0;0>(get(psk1), get(psk2), 0x) in
    let k = kdf<0;0;0>(ck1, get(psk3), 0x) in
    let c = aenc(k, get(d)) in 
    output c
    


