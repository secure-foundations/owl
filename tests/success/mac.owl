def A_main () @ A 
    requires corr(d) : Unit = 
    let _ = output get(d) to endpoint(B) in
    let t = mac(get(k), get(d)) in 
    output t to endpoint(B)

def B_main () @ B : Unit = 
    input i in 
    input j in 
    corr_case k in 
    case mac_vrfy(get(k), i, j) 
    | None => ()
