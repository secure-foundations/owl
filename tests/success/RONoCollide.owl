locality alice

name n : DH @ alice
name m : DH @ alice

random_oracle L : dh_combine(dhpk(get(n)), get(m)) -> nonce

def tst() @ alice : Unit = 
    input h in 
    let lem1 = assume (is_group_elem(h)) in 
    let lem2 = assume (! (h = dhpk(get(n)))) in
    assert (! (dh_combine(h, get(m)) = dh_combine(dhpk(get(n)), get(m))));
    let x = hash<L>(dh_combine(h, get(m))) in 
    output x
    
